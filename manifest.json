{
	"name": "PowerFone",
	"version": "0.7",
	"manifest_version": 2,
	"description": " Lista Telef√¥nica Aumentada",
  	"icons": {
		"16": "powerfone16.png",
		"48": "powerfone48.png",
		"128": "powerfone128.png"
	},
	"content_scripts": [{
        "matches": [
			"http://*/*",
			"file:///*/*"
		],

		"include_globs": [
			"*/cata*",
			"*/listel.nsf/*"
		],
		"js": [
			"./lib/jquery-1.8.0.min.js",
			"./lib/jquery-ui-1.8.23.min.js",
            "./lib/jquery.qtip-1.0.0-rc3.min.js",
            "./lib/jquery.qrcode.min.js",

			"./lib/mustache.js",
			"./lib/jade.min.js",

			"powerfone.user.js",

			"./lib/less-1.3.0.min.js"
		]
	}],
	"web_accessible_resources": [
		"images/telefone.png",
		"images/local.png",
		"images/qrcode.png",
		"images/no_picture.png",
		"css/powerFone.less",
		"pessoa.mustache",
		"pessoa.jade"
	]
}